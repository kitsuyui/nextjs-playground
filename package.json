{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-all": "concurrently \"pnpm:format:check\" \"pnpm:lint\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "concurrently \"pnpm:test:e2e\"",
    "test:e2e": "cypress run",
    "format": "concurrently \"pnpm:format:write\"",
    "format:check": "prettier --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "format:write": "prettier --write --ignore-path .gitignore --ignore-path .prettierignore ."
  },
  "dependencies": {
    "@types/node": "18.17.5",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.16",
    "next": "13.4.16",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cypress": "^12.17.3",
    "prettier": "^3.0.2"
  }
}
